
<!--ユーザー詳細-->
<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(80,80) %>
  <% if @user == current_user %>
    <p><%= link_to "edit", edit_user_path(@user) %></p>
  <% end %>
</div>


<h1>Users</h1>
<!--ユーザー一覧-->
<% @books.each do |book| %>
<div>
  <p>image <%= image_tag book.user.get_profile_image(80,80) %></p>
  <p>name <%= book.title %></p>
 
  <td><%= link_to "Show", book_path(book.id) %></td>
  
</div>
<% end %>